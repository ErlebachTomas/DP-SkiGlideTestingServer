{
  "name": "dp-ski-glide-testing-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app",
    "dev": "nodemon app.js",
    "doc": "node doc.js",
    "test": "jest --verbose ./test --detectOpenHandles --runInBand --forceExit --setupFiles dotenv/config"
  },
  "jest": {
    "collectCoverage": true,
    "reporters": [
      "default",
