  "author": {
    "name": "tomas erlebach"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "debug": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "googleapis": "^99.0.0",
    "mathjs": "^11.7.0",
    "ml-distance": "^4.0.0",
    "ml-regression-multivariate-linear": "^2.0.4",
    "mongoose": "^6.10.4",
    "morgan": "^1.10.0",
    "pug": "^2.0.4",
    "serve-favicon": "^2.5.0",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
