{
  "name": "dp-ski-glide-testing-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app",
    "dev": "nodemon app.js",
    "doc": "node doc.js",
    "test": "jest --verbose ./test --detectOpenHandles --runInBand --forceExit --setupFiles dotenv/config"
  },
  "jest": {
    "collectCoverage": true,
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./test_report",
          "filename": "report.html",
          "openReport": true
        }
      ]
    ]
  },
  "description": "DP-SkiGlideTestingServer",
  "author": {
    "name": "tomas erlebach"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.6",
    "debug": "^2.2.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "googleapis": "^99.0.0",
    "jest": "^29.0.3",
    "jest-html-reporters": "^3.0.11",
    "mongoose": "^6.5.4",
    "morgan": "^1.10.0",
    "pug": "^2.0.4",
    "serve-favicon": "^2.5.0",
    "supertest": "^6.2.4",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest-html-reporter": "^3.7.0"
  }
}
